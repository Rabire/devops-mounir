{
  "extends": ["@commitlint/config-conventional"],

  "rules": {
    "type-enum": [
      2,
      "always",
      [
        "feat",
        "fix",
        "config",
        "style",
        "refactor",
        "docs",
        "test",
        "ci",
        "nginx",
        "docker"
      ]
    ]
  }
}
